<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: e3e2e3;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="airvia.airvialtd.OfficeManagerController">

    <Label layoutX="56.0" layoutY="87.0" text="AirVia Ltd" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="50.0">
        <font>
            <Font name="Comic Sans MS" size="24.0" />
        </font>
    </Label>
   <FlowPane prefHeight="200.0" prefWidth="200.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="100.0">
      <children>
            <Button fx:id="homeButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#homeButtonClick" text="Home">
                <font>
                    <Font name="Comic Sans MS" size="24.0" />
                </font>
            <FlowPane.margin>
               <Insets />
            </FlowPane.margin>
            </Button>
         <Button fx:id="ticketButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#ticketButtonClick" text="Ticket">
            <font>
               <Font name="Comic Sans MS" size="24.0" />
            </font>
         </Button>
            <Button fx:id="blanksButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#blankButtonClick" text="Blanks">
                <font>
                    <Font name="Comic Sans MS" size="24.0" />
                </font>
            </Button>
            <Button fx:id="reportButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#reportButtonClick" text="Report">
                <font>
                    <Font name="Comic Sans MS" size="24.0" />
                </font>
            </Button>
            <Button fx:id="settingButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#settingButtonClick" text="Setting">
                <font>
                    <Font name="Comic Sans MS" size="24.0" />
                </font>
            </Button>
      </children>
   </FlowPane>

</AnchorPane>
